    <strong class="block font-medium mb-1">Name:</strong> <%= mr_character.name %> <strong class="block font-medium mb-1">Part:</strong> <%= mr_character.part&.name %> <strong class="block font-medium mb-1">Lookup Type:</strong> <%= mr_character.lookup_type&.name %> <strong class="block font-medium mb-1">Display Option:</strong> <%= mr_character.display_option&.name %> <strong class="block font-medium mb-1">Source Data:</strong> <%= mr_character.source_data&.title %>
