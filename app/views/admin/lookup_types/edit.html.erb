<% content_for :title, "Editing lookup_type" %>

<div class="md:w-2/3 w-full">
  <h1 class="font-bold text-4xl">Editing lookup_type</h1>
  <div><p>app/views/admin/lookup_types/edit.html.erb</p></div>
  <%= render "form", lookup_type: @lookup_type %>

  <%= link_to "Show this lookup_type", admin_lookup_type_path(@lookup_type), class: "rounded-md px-3.5 py-2.5 bg-blue-600 hover:bg-blue-500 text-white font-medium" %>
  <%= link_to "Back to lookup_types", admin_lookup_types_path, class: "rounded-md px-3.5 py-2.5 bg-blue-600 hover:bg-blue-500 text-white font-medium" %>
</div>
