<% content_for :title, "Editing lookup item" %>

<div class="md:w-2/3 w-full">
  <h1 class="font-bold text-4xl">Editing lookup item</h1>
  <div><p>app/views/admin/lookup_items/edit.html.erb</p></div>
  <%= render "form", lookup_item: @lookup_item %>

  <%= link_to "Show this lookup_item", admin_lookup_item_path(@lookup_item), class: "rounded-md px-3.5 py-2.5 bg-blue-600 hover:bg-blue-500 text-white font-medium" %>
  <%= link_to "Back to lookup_items", admin_lookup_items_path, class: "rounded-md px-3.5 py-2.5 bg-blue-600 hover:bg-blue-500 text-white font-medium" %>
</div>