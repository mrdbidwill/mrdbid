<div id="<%= dom_id image_mushroom %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    Image Name: <strong class="block text-orange-500 font-medium mb-1"><%= image_mushroom.image_name %></strong>
  </div>
  <div>
    <strong class="block font-medium mb-1">Mushroom Part:</strong>
    <%= image_mushroom.part.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Image Height:</strong>
    <%= image_mushroom.created_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Image Height:</strong>
    <%= image_mushroom.updated_at %>
  </div>
</div>
